; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Builder",
"language": "GDScript",
"path": "res://Scripts/builder.gd"
}, {
"base": "Node",
"class": "Pathfinding",
"language": "GDScript",
"path": "res://Scripts/pathfinding.gd"
}, {
"base": "KinematicBody2D",
"class": "soldier",
"language": "GDScript",
"path": "res://Scripts/soldier.gd"
} ]
_global_script_class_icons={
"Builder": "",
"Pathfinding": "",
"soldier": ""
}

[application]

config/name="SimpleWebRTC"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

GlobalVar="*res://Scripts/Global/GlobalVar.gd"
OnlineGame="*res://Scripts/Global/Game.gd"
MultiplayerClient="*res://Scripts/Global/MultiplayerClient.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=640
window/size/height=400
window/size/test_width=1280
window/size/test_height=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://webrtc/webrtc.tres" ]

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
